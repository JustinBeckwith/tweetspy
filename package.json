    "typescript": "^2.6.1",
    "gts": "^0.5.1"
  },
  "dependencies": {
    "@google-cloud/debug-agent": "^2.1.2",
    "@google-cloud/logging-bunyan": "^0.7.0",
    "@google-cloud/bigquery": "^1.0.0",
